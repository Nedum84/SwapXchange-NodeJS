    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.1",
    "@types/randomstring": "^1.1.7",
    "@types/sequelize": "^4.28.10",
    "@types/shelljs": "^0.8.9",
    "@types/umzug": "^2.3.2",
    "@types/uuid": "^8.3.1",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.21.4",
