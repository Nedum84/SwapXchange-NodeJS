{
  "name": "swapxchange-nodejs",
  "version": "1.0.0",
  "description": "",
  "//type": "module",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ts-node-dev --poll src/index.ts",
    "//pretest": "npm install",
    "test": "NODE_ENV=test jest --runInBand --no-cache",
    "dev-test": "NODE_ENV=test jest --runInBand --watchAll --no-cache",
    "db:drop": "./node_modules/.bin/ts-node src/database/umzug/drop.ts",
    "db:create": "ts-node src/database/umzug/migrate.ts",
    "db:migrate": "ts-node src/database/umzug/migrate.ts",
    "db:seed:all": "ts-node src/database/umzug/seed.ts",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:reset:all": "npm start && npm run db:reset && npm run db:seed:all",

    "clean": "rm -rf ./build",
    "build": "npm run clean && tsc --build tsconfig.json",
    "postbuild": "cp serverless.yml ./build && cp package.json ./build && cp -R node_modules ./build",
    "deploy": "cd build && npm install --only=prod && sls deploy --stage dev --region eu-west-2 && cd ..",
    "deploy2": "sls deploy --stage dev --region eu-west-2",
    "build-deploy": "npm run build && npm run deploy2",
    "deploy-offline": "sls offline"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "keywords": [],
